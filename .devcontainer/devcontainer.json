{
	"name": "BT-diagrams",
	"build": {
		"dockerfile": "Dockerfile",
		"context": ".."
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-python.vscode-pylance",
				"streetsidesoftware.code-spell-checker"
			]
		}
	},
	"postCreateCommand": "pip install -r requirements.txt",
	"runArgs": [
		"--name",
		"BT-diagrams"
	]
}